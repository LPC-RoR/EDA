@article{ ISI:A1995UN21400003,
Author = {Belkoura, I and Dubois, T and Dubois, J and Vasseur, J},
Title = {{Evidence for callose synthesis around nuclear cells during adventive
   embryogenesis in troyer citrange {[}Citrus sinensis (L) Osbeck x
   Poncirus trifoliata (L) raf]}},
Journal = {{ACTA BOTANICA GALLICA}},
Year = {{1995}},
Volume = {{142}},
Number = {{5}},
Pages = {{425-432}},
Abstract = {{Callose has been characterized in the cell walls of initial cells of
   nucellar embryoids that develop in site in a polyembryonic hybrid of
   Citrus (Citrus sinensis x Poncirus trifoliata). Callose is laid after
   differentiation of the megaspore mother cell, which is also surrounded
   by a thick callosic wall, and before flower opening. In a monoembryonic
   species (Citrus limon) the megaspore mother cell is the only nucellar
   cell surrounded by callose. This could emphasize the role of callose in
   isolation of cells that engage in a new morphogenetic path. This beta
   (1-3) glucan can be used as a precocious marker of nucellar
   embryogenesis in situ and of somatic embryogenesis in vitro.}},
