@article{ ISI:A1995QR11900034,
Author = {AMBROSE, JT and SCHULTHEIS, JR and BAMBARA, SB and MANGUM, W},
Title = {{AN EVALUATION OF SELECTED COMMERCIAL BEE ATTRACTANTS IN THE POLLINATION
   OF CUCUMBERS AND WATERMELONS}},
Journal = {{AMERICAN BEE JOURNAL}},
Year = {{1995}},
Volume = {{135}},
Number = {{4}},
Pages = {{267-272}},
Month = {{APR}},
Abstract = {{Two commercial honey bee (Apis Mellifera L.) attractants were evaluated
   to determine their effectiveness in improving cucumber and watermelon
   pollination and subsequent yield.  BeeLine(R), a honey bee feeding
   stimulant, was tested in watermelon {[}Citrullus lanatus (Thunb.) Matson
   \& Nakai] production; and Bee-Scent(R), a worker honey bee attractant,
   was tested in both cucumber (Cucumis sativus L.) and watermelon
   production.  Neither product significantly increased bee activity on the
   subject vine crops when compared to an untreated control nor did they
   increase the value of the subsequent harvest.  Based on this study, the
   authors recommend that growers should invest financial resources in
   renting additional colonies of bees for pollination of cucumbers and
   watermelons rather than investing in commercial honey been attractants.}},
