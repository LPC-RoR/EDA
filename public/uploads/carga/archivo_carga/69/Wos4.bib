@article{ ISI:A1993LE26200016,
Author = {MACHUKA, JS and WAITHAKA, K and GOPALAN, HNB},
Title = {{EMBRYO CULTURE AND GEL-ELECTROPHORETIC IDENTIFICATION OF NUCELLAR AND
   ZYGOTIC SEEDLINGS OF CITRUS-LIMON (L) AND CITRUS-SINENSIS (L)}},
Journal = {{DISCOVERY AND INNOVATION}},
Year = {{1993}},
Volume = {{5}},
Number = {{1}},
Pages = {{75-80}},
Month = {{MAR}},
Abstract = {{Five enzyme systems: peroxidase, esterase, glutamate oxaloacetate
   transaminase, malate dehydrogenase and malic were assayed in Citrus
   limon (rough lemon) and Citrus sinensis (Valencia) by polyacrylamide gel
   electrophoresis. Isozyme profiles were used to distinguish nucellar from
   zygotic seedlings produced from in vitro culture of preformed embryos.
   The frequency of zygotic seedlings resulting from open-pollination
   varied between 0.0\% and 9.0\%. Embryo culture significantly enhanced
   the recovery of zygotic seedlings in Rough lemon but not in Valencia.
   Plantlets produced via in vitro nucellar embryogenesis were genetically
   uniform. The isolation and culture of preformed embryos resulted in the
   production of up to 13 and 9 seedlings per seed in Valencia and Rough
   lemon, respectively. The use of tissue culture and isozymes as tools in
   citrus breeding is discussed.}},
