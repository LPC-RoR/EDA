@article{ ISI:000282094100005,
Author = {Iriel, Analia and Gabriela Lagorio, Maria},
Title = {{Is the flower fluorescence relevant in biocommunication?}},
Journal = {{NATURWISSENSCHAFTEN}},
Year = {{2010}},
Volume = {{97}},
Number = {{10}},
Pages = {{915-924}},
Month = {{OCT}},
Abstract = {{Flower fluorescence has been previously proposed as a potential visual
   signal to attract pollinators. In this work, this point was addressed by
   quantitatively measuring the fluorescence quantum yield (I broken
   vertical bar (f)) for flowers of Bellis perennis (white, yellow, pink,
   and purple), Ornithogalum thyrsoides (petals and ovaries), Limonium
   sinuatum (white and yellow), Lampranthus productus (yellow), Petunia
   nyctaginiflora (white), Bougainvillea spectabilis (white and yellow),
   Antirrhinum majus (white and yellow), Eustoma grandiflorum (white and
   blue), Citrus aurantium (petals and stigma), and Portulaca grandiflora
   (yellow). The highest values were obtained for the ovaries of O.
   thyrsoides (I broken vertical bar (f) = 0.030) and for Citrus aurantium
   petals (I broken vertical bar (f) = 0.014) and stigma (I broken vertical
   bar (f) = 0.013). Emitted photons as fluorescence were compared with
   reflected photons. It was concluded that the fluorescence emission is
   negligible compared to the reflected light, even for the most
   fluorescent samples, and it may not be considered as an optical signal
   in biocommunication. The work was complemented with the calculation of
   quantum catches for each studied flower species to describe the visual
   sensitization of eye photoreceptors.}},
