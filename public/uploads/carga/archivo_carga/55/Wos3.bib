@inproceedings{ ISI:000166400700020,
Author = {Koltunow, AM and Vivian-Smith, A and Sykes, SR},
Editor = {{Goren, R and Goldschmidt, EE}},
Title = {{Molecular and conventional breeding strategies for seedless Citrus}},
Booktitle = {{PROCEEDINGS OF THE FIRST INTERNATIONAL SYMPOSIUM ON CITRUS BIOTECHNOLOGY}},
Series = {{ACTA HORTICULTURAE}},
Year = {{2000}},
