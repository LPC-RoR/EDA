@article{ ISI:000235407000009,
Author = {Hasegawa, M and Yahara, T and Yasumoto, A and Hotta, M},
Title = {{Bimodal distribution of flowering time in a natural hybrid population of
   daylily (Hemerocallis fulva) and nightlily (Hemerocallis citrina)}},
Journal = {{JOURNAL OF PLANT RESEARCH}},
Year = {{2006}},
Volume = {{119}},
Number = {{1}},
Pages = {{63-68}},
Month = {{JAN}},
Abstract = {{Time of flower anthesis in a day is thought to evolve in response to the
   time of pollinator activities. We studied blooming and withering time in
   natural populations of daylily (Hemerocallis fulva), nightlily
   (Hemerocallis citrina) and their hybrids, and also in an artificially
   obtained array of the F1 hybrids. Blooming time of H. fulva varied from
   4:30 to 7:30 and H. citrina varied from 16:30 to 20:30. In a natural
   hybrid population, blooming time and withering time showed discontinuous
   bimodal distribution in spite that morphological traits of flowers
   showed continuous unimodal variation. Most F1 hybrids showed diurnal
   flowering. These findings indicate that only a few genes have strong
   phenotypic effect on the determination of flowering time in
   Hemerocallis, and suggest that the evolution from a H. fulva-like
   ancestor to H. citrina was not a continuous process by accumulation of
   minute mutations.}},
