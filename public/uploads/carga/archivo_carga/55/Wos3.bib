@article{ ISI:000282056900007,
Author = {da Silva, Ernani Clarete and Maciel, Gabriel Mascarenhas},
Title = {{SOYBEAN GENE FLOW IN THE SOUTH REGION OF MINAS GERAIS STATE, BRAZIL}},
