@inproceedings{ ISI:000180072100012,
Author = {Watanabe, S and Nakano, Y and Okano, K and Sugiyama, K and Morishita, M},
Editor = {{Nishimura, S and Ezura, H and Matsuda, T and Tazuke, A}},
Title = {{Effect of cropping season on the formation of empty seeds in seedless
   watermelon fruits produced by soft-X-irradiated pollen}},
Booktitle = {{PROCEEDINGS OF THE IIND INTERNATIONAL SYMPOSIUM ON CUCURBITS}},
Series = {{ACTA HORTICULTURAE}},
Year = {{2002}},
Number = {{588}},
Pages = {{89-92}},
Note = {{2nd International Symposium on Cucurbits, TSUKUBA, JAPAN, SEP 28-OCT 01,
   2001}},
Organization = {{Int Soc Hort Sci; Japanese Soc Hort Sci}},
Abstract = {{A new method of producing seedless watermelon fruits by using pollen
   irradiated with soft-X-rays has been developed recently. This method
   enables seedless fruits to be produced from any diploid cultivar. To
   investigate how cropping season affects the formation of empty seeds, we
   compared July-harvesting culture (JHC) and December-harvesting culture
   (DHC) in 1999 using `Shimaou max RE' and in 2000 using `Fujihikari'. We
   also examined the effects of pollination time during the day and
   night-time temperature during the fruit development period on the
   formation of empty seeds. Empty seeds were categorized as
   semitransparent, white, or brown. Pollination time influenced neither
   the number nor the type of empty seeds. The percentage of fruits with
   brown empty seeds was higher in JHC than in DHC. The proportion of white
   empty seeds to total empty seeds tended to be higher in DHC than in JHC.
   The air temperature during the fruit development period was considerably
   higher in JHC than in DHC. The percentage of fruits that had brown empty
   seeds was higher at higher night-time temperature. We conclude that the
   formation of empty seeds is influenced by cropping season, and that high
   temperature during fruit development stimulates the formation of brown
   empty seeds, which lower the fruit quality of seedless watermelons.}},
