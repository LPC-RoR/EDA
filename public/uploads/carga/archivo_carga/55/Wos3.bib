Title = {{The dynamics of social-ecological systems in urban landscapes -
   Stockholm and the National Urban Park, Sweden}},
Booktitle = {{URBAN BIOSPHERE AND SOCIETY: PARTNERSHIP OF CITIES}},
Series = {{Annals of the New York Academy of Sciences}},
Year = {{2004}},
Volume = {{1023}},
