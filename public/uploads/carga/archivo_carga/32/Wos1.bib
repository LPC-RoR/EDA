@article{ ISI:000483404700010,
Author = {Kim, Taebok and Lim, Jeonghyeon and Jeong, Juhyung and Seong, Munho and
   Kim, Kabcheol and Jeon, HyongGwon and Noh, Jaejong},
Title = {{Production of Diploid Seedless Watermelon Using Tetraploid Watermelon
   Pollen}},
Journal = {{HORTICULTURAL SCIENCE \& TECHNOLOGY}},
Year = {{2019}},
Volume = {{37}},
Number = {{4}},
Pages = {{520-527}},
Abstract = {{Diploid seedless watermelon is produced using soft X-ray irradiated
   pollen, but production costs are high. This study examined whether soft
   X-ray-irradiated pollen can be replaced by tetraploid watermelon pollen.
   The fruit set rate of diploid seedless watermelon differed from that of
   each tetraploid watermelon line and showed a high correlation with the
   number of watermelon seeds produced by self-pollination. The fruit set
   rate was lower when pollens from tetraploid plants were used than when
   soft X-ray-irradiated pollen was used, but the qualities of watermelons
   produced were similar. The pollen tube germination rate of tetraploid
   plants was lower than diploid ones in vivo and in vitro. The shape of
   tetraploid watermelon pollen was incomplete and uneven compared to that
   of diploid pollen. Most tetraploid watermelon pollen had no pore, but
   some had as many as four pores. These results suggest that production of
   diploid seedless watermelon requires a tetraploid with normal
   development and high pollen tube germination rate.}},
