@article{ ISI:000392703700003,
Author = {Babazadeh-Darjazi, Behzad},
Title = {{Study on peel components and juice quality of three mandarin hybrids
   (Citrus reticulata) on sour orange rootstock cultivated in Ramsar, Iran}},
Journal = {{BOLETIN LATINOAMERICANO Y DEL CARIBE DE PLANTAS MEDICINALES Y AROMATICAS}},
Year = {{2017}},
Volume = {{16}},
Number = {{2}},
Pages = {{110-120}},
Month = {{MAR}},
Abstract = {{The peel components and juice quality of three mandarin hybrids were
   investigated in this study. Peel components were extracted using the
   cold-press method and analyzed using GC-FID and GC-MS. Total soluble
   solids, total acid, pH value, ascorbic acid as well as density were
   determined in juice obtained from mandarin hybrids. Twenty-six,
   thirty-five and nineteen peel components were identified in Fortune,
   Robinson and Osceola respectively including: aldehydes, alcohols,
   esters, monoterpenes, sesquiterpenes and other components. The major
   components were limonene, gamma-terpinene, (E)-beta-ocimene,
   beta-myrcene, sabinene, linalool and alpha-Pinene. Among the three
   scions examined, Fortune showed the highest content of aldehydes and
   Robinson showed the highest content of TSS. This study shows that scion
   has a profound influence on aldehyd and TSS that are important to
   quality improvement.}},
