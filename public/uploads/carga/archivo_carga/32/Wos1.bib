@article{ ISI:000508918500001,
Author = {Silva, Paulo Antonio},
Title = {{FLOWER EATING BY THE TOCO TOUCAN (RAMPHASTOS TOCO) IN AN ANTHROPOGENIC
   LANDSCAPE DURING THE DRY SEASON}},
Journal = {{ORNITOLOGIA NEOTROPICAL}},
Year = {{2019}},
Volume = {{30}},
Pages = {{51-55}},
Month = {{APR 18}},
Abstract = {{Toucans rarely feed on plant parts other than ripe fleshy fruits. Here,
   I report observations of flower exploitation by Toco Toucans (Ramphastos
   toco), in Sao Paulo state, Brazil. This toucan fed on the buds, floral
   tissues, whole flowers and nectar of seven plant species. I made the
   observations in a highly anthropogenic landscape, mostly at the height
   of the dry season, when the availability of ripe fleshy fruits declines
   in the area. The exploitation of flowers reported here is consistent
   with an opportunistic foraging habit in which toucans resort to eating
   flowers to obtain nutrients, energy, and water when ripe fleshy fruits
   are scarce.}},
