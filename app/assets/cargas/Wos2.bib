@inproceedings{ ISI:000333274200077,
Author = {Adetula, O. A. and Chikaleke, V. A.},
Editor = {{Hannweg, K and Penter, M}},
Title = {{Development of Improved Cultivars of Telfairia occidentalis for Improved
   Livelihood and Food Security}},
Booktitle = {{II ALL AFRICA HORTICULTURE CONGRESS}},
Series = {{Acta Horticulturae}},
Year = {{2013}},
Volume = {{1007}},
Pages = {{665-670}},
