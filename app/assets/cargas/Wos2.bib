@inproceedings{ ISI:000313131400013,
Author = {Esenalieva, A. and Drewes-Alvarez, R. and Arnold, R. and Wiedemann, M.
   and Meinl, K. and Pohlheim, F. and Hofmann, C. and Olbricht, K.},
Editor = {{Orlikowska, T}},
Title = {{Interspecific Hybridisation between Pelargonium zonale Hybrids and
   Pelargonium tongaense Vorster on the Tetraploid Ploidy Level}},
Booktitle = {{XXIV INTERNATIONAL EUCARPIA SYMPOSIUM SECTION ORNAMENTALS: ORNAMENTAL
   BREEDING WORLDWIDE}},
