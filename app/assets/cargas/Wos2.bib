Title = {{Tentative application of an electronic nose to the study of the
   parentage of Romanian grape varieties Sarba and Alb Aromat}},
Booktitle = {{CONFERENCE AGRICULTURE FOR LIFE, LIFE FOR AGRICULTURE}},
Series = {{Agriculture and Agricultural Science Procedia}},
