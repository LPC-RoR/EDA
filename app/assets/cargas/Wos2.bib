@article{ ISI:000384395000007,
Author = {Han, Eun-Ju and Popova, Elena and Cho, Gyu-Taek and Park, Sang-Un and
   Lee, Sheong-Chun and Pritchard, Hugh W. and Kim, Haeng-Hoon},
Title = {{POST-HARVEST EMBRYO DEVELOPMENT IN GINSENG SEEDS INCREASES DESICCATION
