<p id="notice"><%= notice %></p>

<h1>Columnas</h1>

<table>
  <thead>
    <tr>
      <th>Orden</th>
      <th>Linea</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @columnas.each do |columna| %>
      <tr>
        <td><%= columna.orden %></td>
        <td><%= columna.linea_id %></td>
        <td><%= link_to 'Show', columna %></td>
        <td><%= link_to 'Edit', edit_columna_path(columna) %></td>
        <td><%= link_to 'Destroy', columna, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Columna', new_columna_path %>
