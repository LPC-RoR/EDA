<p id="notice"><%= notice %></p>

<h1>Metodologias</h1>

<table>
  <thead>
    <tr>
      <th>Metodologia</th>
      <th>Orden</th>
      <th>Publicacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @metodologias.each do |metodologia| %>
      <tr>
        <td><%= metodologia.metodologia %></td>
        <td><%= metodologia.orden %></td>
        <td><%= metodologia.publicacion_id %></td>
        <td><%= link_to 'Show', metodologia %></td>
        <td><%= link_to 'Edit', edit_metodologia_path(metodologia) %></td>
        <td><%= link_to 'Destroy', metodologia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Metodologia', new_metodologia_path %>
