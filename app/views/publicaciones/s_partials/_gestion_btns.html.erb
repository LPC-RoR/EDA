<%= render partial: '0p/tabla/btn', locals: {texto: "Selección", link: publicaciones_path(html_options: {tab: 'Selección'}), delete: false, clr: 'dark' }  %>
<%= render partial: '0p/tabla/btn', locals: {texto: "Proceso", link: publicaciones_path(html_options: {tab: 'Proceso'}), delete: false, clr: 
 'dark' }  %>

<% unless @carpetas_seleccion.blank? %>
	<% if @carpetas_seleccion.any? %>
 		<% @carpetas_seleccion.each do |carpeta| %>
 			<%= render partial: '0p/tabla/btn', locals: {texto: carpeta.carpeta, link: "/carpetas/#{carpeta.id}/btn_asigna?publicacion_id=#{@objeto.id}", delete: false, clr: 'success' }  %>
 		<% end %>
	<% end %>
<% end %>

</br></br>

<%# if @objeto.origen == 'ingreso' %>
	<%# (Publicacion::DOC_TYPES - [@objeto.doc_type]).each do |doc_type| %>
		<%#= render partial: '0p/tabla/btn', locals: {texto: "#{doc_type}", link: "/publicaciones/cambia_tipo?publicacion_id=#{@objeto.id}&doc_type=#{doc_type}", delete: false, clr: c_color(controller_name) }  %>
	<%# end %>
<%# end %>
