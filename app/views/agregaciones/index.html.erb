<p id="notice"><%= notice %></p>

<h1>Agregaciones</h1>

<table>
  <thead>
    <tr>
      <th>Categoria</th>
      <th>Concepto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agregaciones.each do |agregacion| %>
      <tr>
        <td><%= agregacion.categoria_id %></td>
        <td><%= agregacion.concepto_id %></td>
        <td><%= link_to 'Show', agregacion %></td>
        <td><%= link_to 'Edit', edit_agregacion_path(agregacion) %></td>
        <td><%= link_to 'Destroy', agregacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agregacion', new_agregacion_path %>
