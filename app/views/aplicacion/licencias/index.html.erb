<p id="notice"><%= notice %></p>

<h1>Licencias</h1>

<table>
  <thead>
    <tr>
      <th>N meses</th>
      <th>Perfil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @licencias.each do |licencia| %>
      <tr>
        <td><%= licencia.n_meses %></td>
        <td><%= licencia.perfil_id %></td>
        <td><%= link_to 'Show', licencia %></td>
        <td><%= link_to 'Edit', edit_licencia_path(licencia) %></td>
        <td><%= link_to 'Destroy', licencia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Licencia', new_licencia_path %>
