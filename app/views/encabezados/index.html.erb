<p id="notice"><%= notice %></p>

<h1>Encabezados</h1>

<table>
  <thead>
    <tr>
      <th>Orden</th>
      <th>Encabezado</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encabezados.each do |encabezado| %>
      <tr>
        <td><%= encabezado.orden %></td>
        <td><%= encabezado.encabezado %></td>
        <td><%= encabezado.tipo %></td>
        <td><%= link_to 'Show', encabezado %></td>
        <td><%= link_to 'Edit', edit_encabezado_path(encabezado) %></td>
        <td><%= link_to 'Destroy', encabezado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encabezado', new_encabezado_path %>
