<p id="notice"><%= notice %></p>

<h1>Caracterizaciones</h1>

<table>
  <thead>
    <tr>
      <th>Caracterizacion</th>
      <th>Proyecto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @caracterizaciones.each do |caracterizacion| %>
      <tr>
        <td><%= caracterizacion.caracterizacion %></td>
        <td><%= caracterizacion.proyecto_id %></td>
        <td><%= link_to 'Show', caracterizacion %></td>
        <td><%= link_to 'Edit', edit_caracterizacion_path(caracterizacion) %></td>
        <td><%= link_to 'Destroy', caracterizacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caracterizacion', new_caracterizacion_path %>
