<% if a == 'seleccion' %>
	<%= render partial: '0p/tabla/btn_seleccion', locals: {link_seleccion: "#{@link_seleccion}#{objeto.id}" } %>
<% else %>
	<% if despliega_btns?(objeto) %>
		<%= render partial: '0p/tabla/btns_e', locals: {objeto: objeto} %>
		<!-- BOTONES ESTADO : SOLO -->
		<%= render partial: '0p/tabla/btns_estado', locals: {objeto: objeto, estados: objeto.class.name.constantize::ESTADOS, link_estado: "/#{objeto.class.name.tableize}/#{objeto.id}/estado?estado="} if d_tabla(objeto.class.name.tableize, :estados) %>
	<% end %>
<% end %>
