<!-- Pregunta si esta acción despliega TABS, si es así los despliega -->
<% if r_tabs?(action_name) %>
	<%= render partial: '0p/tabs/tabs', locals: {tabs: m_tabs(controller_name), link_tabs: get_link(controller_name, 'ftab'), active: @ftab } %>
<% end %>

<!-- Pregunta si esta acción despliega una TABLA, si es asi la despliega -->
<% if r_tabla?(action_name) %>
	<%= render partial: '0p/tabla/tabla', locals: {c: alias_tabla(@ftab), a: 'index'} %>
<% end %>
