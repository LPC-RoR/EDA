<% if Configuracion::S_LINKS_MODELS.include?(o.class.name) %>
	<% if o.show_links.map {|ll| ll[2]}.include?(true) %>
		<% o.show_links.each do |l| %>
			<%= render partial: '0p/general/btn', locals: {texto: l[0], link: l[1], delete: false, style: 'btn-outline-dark'} if l[2] %>
		<% end %>
		</br></br>
	<% end %>
<% end %>

<% if Configuracion::S_BT_LINKS_MODELS.include?(o.class.name) %>
	<% unless o.class::S_BT_LINKS_OBJECTS.map {|mbt| o.send(mbt.downcase)}.compact.empty? %>
		<% o.class::S_BT_LINKS_OBJECTS.each do |bt| %>
			<% o_bt = o.send(bt.downcase) %>
			<%= render partial: '0p/general/btn', locals: {texto: show_title(o_bt), link: o_bt, delete: false, style: 'btn-outline-dark' } unless o_bt.blank? %>
		<% end %>
		</br></br>
	<% end %>
<% end %>

<% if Configuracion::S_HMT_LINKS_MODELS.include?(o.class.name) %>
	<% unless o.class::S_HMT_LINKS_COLLECTIONS.map {|mhtm| o.send(mhtm)}.compact.empty? %>
		<% o.class::S_HMT_LINKS_COLLECTIONS.each do |hmt| %>
			<% coleccion = o.send(hmt) %>
			<% coleccion.each do |l| %>
				<%= render partial: '0p/general/btn', locals: {texto: show_title(l), link: l, delete: false, style: 'btn-outline-dark' } %>
			<% end %>
		<% end %>
		</br></br>
	<% end %>
<% end %>
