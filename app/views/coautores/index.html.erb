<p id="notice"><%= notice %></p>

<h1>Coautores</h1>

<table>
  <thead>
    <tr>
      <th>Proyecto</th>
      <th>Perfil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coautores.each do |coautor| %>
      <tr>
        <td><%= coautor.proyecto_id %></td>
        <td><%= coautor.perfil_id %></td>
        <td><%= link_to 'Show', coautor %></td>
        <td><%= link_to 'Edit', edit_coautor_path(coautor) %></td>
        <td><%= link_to 'Destroy', coautor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coautor', new_coautor_path %>
